  "dependencies": {
    "classcat": "5.0.3",
    "next": "10.2.3",
    "next-themes": "0.0.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "0.5.6"
  },
  "devDependencies": {
    "@headlessui/react": "1.2.0",
    "@tailwindcss/jit": "0.1.18",
    "@tailwindcss/typography": "0.4.1",
    "@testing-library/react": "11.2.7",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.1",
    "@types/react": "17.0.8",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "autoprefixer": "10.2.6",
