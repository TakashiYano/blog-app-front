    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tailwind": "0.2.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.3",
    "jest-watch-typeahead": "0.6.4",
    "msw": "0.29.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.0",
    "prettier": "2.3.0",
    "react-textarea-autosize": "8.3.2",
    "tailwindcss": "2.1.2",
    "typescript": "4.3.2"
  },
  "msw": {
    "workerDirectory": "public"
